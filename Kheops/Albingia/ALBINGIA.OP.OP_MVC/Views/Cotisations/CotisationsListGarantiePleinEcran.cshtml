@using ALBINGIA.OP.OP_MVC.Models.ModelesCotisations
@model ModeleInfoGarantie
<div class="GradientSection DivListGranties">
    <div id="divHeaderGaranties" style="text-align: center;" >
        <table style="width:1005px;margin-left:10px;" class="trHeight">
            <tr>
                <td>
                    <table id="tblHeaderGaranties" style="width: 980px;" class="trHeight">
                        <tr style="text-align: center;">
                            <td class="tablePersoHead" style="width: 159px;">Garantie
                            </td>
                            <td class="tablePersoHead" style="width: 22px;">R
                            </td>
                            <td class="tablePersoHead" style="width: 23px;">F
                            </td>
                            <td class="tablePersoHead" style="width: 82px;">Tarif
                            </td>
                            <td class="tablePersoHead" style="width: 118px;">
                                <table width="100%">
                                    <tr>
                                        <td colspan="2" valign="bottom">LCI
                                        </td>
                                    </tr>
                                    <tr>
                                        <td valign="top">Valeur
                                        </td>
                                        <td valign="top">U
                                        </td>
                                    </tr>
                                </table>
                            </td>
                            <td class="tablePersoHead" style="width: 118px;">
                                <table width="100%">
                                    <tr>
                                        <td colspan="2" valign="bottom">Assiette
                                        </td>
                                    </tr>
                                    <tr>
                                        <td valign="top">Valeur
                                        </td>
                                        <td valign="top">U
                                        </td>
                                    </tr>
                                </table>
                            </td>
                            <td class="tablePersoHead" style="width: 118px;">
                                <table width="100%">
                                    <tr>
                                        <td colspan="2" valign="bottom">Taux
                                        </td>
                                    </tr>
                                    <tr>
                                        <td valign="top">Valeur
                                        </td>
                                        <td valign="top">U
                                        </td>
                                    </tr>
                                </table>
                            </td>
                            <td class="tablePersoHead" style="width: 290px;">
                                <table width="100%">
                                    <tr>
                                        <td colspan="3" valign="bottom">Cotisation
                                        </td>
                                    </tr>
                                    <tr>
                                        <td valign="top" style="width: 85px">HT
                                        </td>
                                        <td valign="top" style="width: 85px">Taxe
                                        </td>
                                        <td valign="top" style="width: 85px">TTC
                                        </td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td>
                    <div style="width: 100%; overflow: auto;" class="divHeightCotisationPleinEcran">
                        <table id="tblGarantieListFullScreen" style="width: 980px;" class="trHeight">
                            @foreach (var garantie in Model.GarantiesFS)
                            {
                                <tr>
                                    <td style="width: 156px; text-align: left; padding-left: 5px;" title="@(garantie.CodeGarantie + " - " + garantie.NomGarantie)">
                                        @Html.Hidden($"Hidden¤{garantie.CodeGarantie}", string.Empty, new { id = $"Hidden¤{garantie.CodeGarantie}" })
                                        @Html.Hidden($"CodeGarantie¤{garantie.CodeGarantie}", garantie.CodeGarantie, new { id = $"CodeGarantie¤{garantie.CodeGarantie}" })
                                        @garantie.CodeGarantie
                                    </td>
                                    <td style="width: 24px;">@garantie.CodeRisque
                                    </td>
                                    <td style="width: 24px;">@garantie.CodeFormule
                                    </td>
                                    <td id="tarif¤@(garantie.CodeGarantie)" style="width: 85px;" class="CursorPointer">
                                        @Html.Hidden($"Tarif¤{garantie.CodeGarantie}", garantie.Tarif, new { id = $"Tarif¤{garantie.CodeGarantie}" })
                                        @Html.Hidden($"Kpgaran¤{garantie.CodeGarantie}", garantie.LienKpgaran, new { id = $"Kpgaran¤{garantie.CodeGarantie}" })
                                        @garantie.Tarif
                                    </td>
                                    <td style="width: 85px; text-align: right; padding-right: 5px;">
                                        <span albmask="decimal">@garantie.LCIValeur</span>
                                    </td>
                                    <td style="width: 25px;">@garantie.LCIUnite</td>
                                    <td style="width: 85px; text-align: right; padding-right: 5px;">
                                        <span albmask="decimal">@garantie.AssietteValeur</span>
                                    </td>
                                    <td style="width: 25px;">@garantie.AssietteUnite</td>
                                    <td style="width: 85px; text-align: right; padding-right: 5px;">
                                        <span albmask="decimal">@garantie.TauxValeur</span>
                                    </td>
                                    <td style="width: 25px;">@garantie.TauxUnite</td>
                                    <td style="width: 90px; text-align: right; padding-right: 5px;">
                                        <span albmask="decimal">@garantie.CotisationHT</span>
                                    </td>
                                    <td style="width: 90px; text-align: right; padding-right: 5px;">
                                        <span albmask="decimal">@garantie.CotisationTaxe</span>
                                    </td>
                                    <td style="width: 90px; text-align: right; padding-right: 5px;">
                                        <span albmask="decimal">@garantie.CotisationTTC</span>
                                    </td>
                                </tr>
                            }
                        </table>
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <table style="width: 980px;" class="trHeight">
                        <tr>
                            <td style="font-weight: bold; text-align: right; padding-right: 5px;">Sous-Total
                            </td>
                            <td style="width: 25px;">&nbsp;
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px; background-color: White;">
                                <span albmask="decimal">@Model.SubCotisationHT</span>
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px; background-color: White;">
                                <span albmask="decimal">@Model.SubCotisationTaxe</span>
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px; background-color: White;">
                                <span albmask="decimal">@Model.SubCotisationTTC</span>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td>
                    <table id="tblTotalPleinEcran" style="width: 980px;" class="trHeight">
                        <tr>
                            <td style="width: 427px; text-align: left; padding-left: 5px; font-weight: bold;">CATNAT
                            </td>
                            <td style="width: 85px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Catnat.AssietteValeur</span>
                            </td>
                            <td style="width: 25px;">@Model.Catnat.AssietteUnite
                            </td>
                            <td style="width: 85px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Catnat.TauxValeur</span>
                            </td>
                            <td style="width: 25px;">@Model.Catnat.TauxUnite
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Catnat.CotisationHT</span>
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Catnat.CotisationTaxe</span>
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Catnat.CotisationTTC</span>
                            </td>
                        </tr>
                        <tr>
                            <td style="width: 427px; text-align: left; padding-left: 5px; font-weight: bold;">Attentat (GAREAT)
                            </td>
                            <td style="width: 85px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal"></span>
                            </td>
                            <td style="width: 25px;"></td>
                            <td style="width: 85px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal"></span>
                            </td>
                            <td style="width: 25px;"></td>
                            <td style="width: 91px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Gareat.CotisationHT</span>
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Gareat.CotisationTaxe</span>
                            </td>
                            <td style="width: 91px; text-align: right; padding-right: 5px;">
                                <span albmask="decimal">@Model.Gareat.CotisationTTC</span>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="GroupFull GroupWithBorder ButtonsHeight divBtnFullScreenFermer">
    <div id="dvLinkClose" class="FloatLeft BtnFullScreenFermer">
        <button id="btnNoFullScreen" type="button" class="CursorPointer FullScreenBtn" title="Fermer le plein écran">
            <img src="@Url.Content("/Content/Images/window_nofullscreen.png")" alt="Fermer" />
        </button>
    </div>
</div>
