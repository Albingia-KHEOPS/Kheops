@model ALBINGIA.OP.OP_MVC.Models.ModelesGestionNomenclature.ModeleNomenclature
<div id="divHeaderGrille">
    <table id="tblHeaderGrille" class="trHeight tableGrille">
        <tr>
            <th class="tablePersoHead thCodeNomen">Code</th>
            <th class="tablePersoHead thLibelleNomen">Libellé</th>
            <th class="tablePersoHead thImgNomen">
                @*<img id="addGrille" name="addGrille" alt="AddGrille" title="Ajouter une grille" src="@Url.Content("/Content/Images/plusajouter1616.png")" />*@
            </th>
        </tr>
    </table>
</div>
<div>
    <table id="tblBodyGrille" class="trHeight tableGrille">
        @foreach (var item in Model.Grilles)
        {
            <tr>
                <td class="CursorPointer tdCodeNomen" title="@item.Code" albinfogrille="@(item.Code)">@item.Code</td>
                <td class="CursorPointer tdLibelleNomen" title="@item.Libelle" albinfogrille="@(item.Code)">@item.Libelle</td>
                <td class="CursorPointer tdImgNomen" albinfogrille="@(item.Code)">&nbsp;</td>
            </tr>
        }
    </table>
</div>
<div id="divGrilleInfo" class="None">
    <div class="overlay-background" style="z-index:10" ></div>
    <div class="dvGrilleInfo">
        <div id="divDataGrilleInfo">
        </div>
    </div>
</div>
