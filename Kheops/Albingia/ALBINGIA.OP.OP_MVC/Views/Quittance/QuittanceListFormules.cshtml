@model ALBINGIA.OP.OP_MVC.Models.ModelesQuittances.ListFormules
<div id="divFormulesHeader" style="text-align: center;">
    <table id="tblFormulesHeader" class="trHeight">
        <tr>
            <th class="col_HeaderFormule tablePersoHead">Formule
            </th>
            <th class="col_HeaderRisque tablePersoHead">Risque
            </th>
            <th class="col_HeaderHTCATNAT tablePersoHead">HT hors CATNAT
            </th>
            <th class="col_HeaderHTCATNAT tablePersoHead">CATNAT
            </th>
            <th class="col_HeaderCATNAT tablePersoHead">Taxes
            </th>
            <th class="col_HeaderTaxes tablePersoHead">TTC
            </th>
            <th class="col_HeaderFinEffet tablePersoHead">Fin d'effet
            </th>
        </tr>
    </table>
</div>
<div id="divFormulesBody">
    <table id="tblFormulesBody" class="trHeight">
        @{
            foreach (var formuleInfo in Model.Formules)
            {
                if (formuleInfo.Ttc != "0" && !string.IsNullOrEmpty(formuleInfo.Ttc))
                {
            <tr>
                <td class="col_Formule" title="@formuleInfo.Libelle">@formuleInfo.Libelle</td>
                <td class="col_Risque" title="@formuleInfo.Risque">@formuleInfo.Risque</td>
                <td class="col_HTCATNAT"><span albmask="decimal">@formuleInfo.HtHorsCatnat</span></td>
                <td class="col_CATNAT"><span albmask="decimal">@formuleInfo.CatNat</span></td>
                <td class="col_Taxes"><span albmask="decimal">@formuleInfo.Taxes</span></td>
                <td class="col_TTC"><span albmask="decimal">@formuleInfo.Ttc</span></td>
                <td class="col_FinEffet" style="text-align: center;">@formuleInfo.FinEffet</td>
            </tr>
                }
            }
        }
    </table>
</div>
