<div id="MessageOffreVerrouillee">
    <div class="overlay-background" style="z-index:14" >
    </div>
    <div id="divDataMessageOffreVerrouillee">
        <table width="99%">
            <tr>
                <td style="background-color: #EDEDED;" colspan="2">
                    <b>&nbsp;Message de sécurité.</b><br />
                </td>
            </tr>
            <tr>
                <td>
                    <img src="/Content/Images/alertes/notif_icn_critical.png" alt="" />
                </td>
                <td style="text-align: center;">
                    @Html.HiddenNoName("idOffreToAcces", string.Empty)
                    <div id="centerOffreVerrouillee"></div>
                </td>
            </tr>
            <tr>

                <td style="text-align: right;" colspan="2">
                    <input type="button" id="btnOVAnnuler" value="Fermer" />
                    <input type="button" id="btnOVReadOnly" value="Consulter" />
                </td>
            </tr>
        </table>
    </div>
</div>

