        <digestAuthentication enabled="false"/>
        <anonymousAuthentication enabled="false"/>
      </authentication>
    </security>
    <directoryBrowse enabled="true"/>
    <httpProtocol allowKeepAlive="true">
      <customHeaders>
        <clear/>
        <add name="X-UA-Compatible" value="IE=EmulateIE11"/>
      </customHeaders>
    </httpProtocol>
  </system.webServer>
