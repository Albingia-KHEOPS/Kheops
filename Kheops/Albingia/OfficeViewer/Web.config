  <system.web>
    <compilation debug="true" targetFramework="4.7"/>
    <httpRuntime targetFramework="4.5"/>
    <globalization fileEncoding="utf-8" requestEncoding="UTF-8" responseEncoding="UTF-8" culture="fr-FR" uiCulture="fr"/>
